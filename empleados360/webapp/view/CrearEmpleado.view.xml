<mvc:View
    controllerName="empleados360.controller.CrearEmpleado.js"
        xmlns="sap.m"
    		xmlns:form="sap.ui.layout.form"
		xmlns:core="sap.ui.core"
		xmlns:u="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"

<Page id="ReviewPage" title="{i18n>resumen}">
								<core:Fragment fragmentName="empleados360.fragment.TipoEmpleado" type="XML"/>
								<footer>
									<Toolbar>
										<ToolbarSpacer></ToolbarSpacer>
										<Button text="{i18n>guardar}" press="onSaveEmployee"></Button>
										<Button text="{i18n>cancelar}" press="onCancel"></Button>
									</Toolbar>
								</footer>
							</Page>
                            </mvc:View>