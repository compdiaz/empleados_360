<mvc:View controllerName="empleados360.ydiazs.HTML5Module.controller.Menu" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
				<Page id="page" title="{i18n>menuPrincipal}">
					<content>
						<Panel headerText="{i18n>empleados}" expandable="true" expanded="true">
							<GenericTile headerImage="sap-icon://add-employee" class="sapUiTinyMarginBegin sapUiTinyMarginTop" press="navToCreateEmployee">
								<TileContent footer="{i18n>crearEmpleado}">
								</TileContent>
							</GenericTile>
							<GenericTile headerImage="sap-icon://employee-lookup" class="sapUiTinyMarginBegin sapUiTinyMarginTop" press="navToShowEmployee">
								<TileContent footer="{i18n>verEmpleados}">
								</TileContent>
							</GenericTile>
						</Panel>
						<Panel headerText="{i18n>pedidos}"  expandable="true" expanded="true">
							<GenericTile id="linkFirmarPedido"  headerImage="sap-icon://signature" class="sapUiTinyMarginBegin sapUiTinyMarginTop" url="https://cf9b8592trial.launchpad.cfapps.us10.hana.ondemand.com/8476915d-b454-4071-9fe3-758baa95e5f1.employees.employees-0.0.1/index.html">
								<TileContent footer="{i18n>firmarPedido}">
								</TileContent>
							</GenericTile>
						</Panel>
					</content>
				</Page>
</mvc:View>
